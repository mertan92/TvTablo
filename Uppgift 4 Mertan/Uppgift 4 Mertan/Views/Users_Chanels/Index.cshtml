@model IEnumerable<Uppgift_4_Mertan.Models.db.Users_Chanels>

@{
    ViewBag.Title = "Lista med dina kanaler";
}

<h2>Lista med dina kanaler</h2>

<p>
    @Html.ActionLink("Lägg till kanal", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Channels.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Users.Username)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Channels.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Users.Username)
        </td>
        <td>
            @Html.ActionLink("Redigera", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Detaljer", "Details", new { id=item.Id }) |
            @Html.ActionLink("Ta bort", "Delete", new { id=item.Id })
        </td>
    </tr>
}
</table>
<div>
    @Html.ActionLink("Tillbaka till Personlig tablå", "PersonligTablo", "Home")
</div>

